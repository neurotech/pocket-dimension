{
  "name": "pocket-dimension",
  "version": "1.0.0",
  "description": "A tiny place to store tiny things.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "buildsrc": "parcel build src/index.html --out-dir build --public-url ./",
    "predeploy": "rm -rf build/ && set NODE_ENV=production&&npm run buildsrc",
    "deploy": "now --public --local-config now.json && now alias --local-config now.json",
    "clean": "now --local-config now.json rm pocket-dimension --safe --yes",
    "test": "node test",
    "watch": "parcel watch src/index.html --out-dir build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neurotech/pocket-dimension.git"
  },
  "author": "Tim Douglas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neurotech/pocket-dimension/issues"
  },
  "homepage": "https://github.com/neurotech/pocket-dimension#readme",
  "dependencies": {
    "alternator": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "cryptr": "^4.0.2",
    "dion": "^2.0.0",
    "dotenv": "^6.1.0",
    "log-symbols": "^2.2.0",
    "request-data": "^1.2.0",
    "righto": "^5.1.0",
    "sea-lion": "^2.0.2",
    "simple-cors": "^1.1.1",
    "tiny-json-http": "^7.0.2",
    "unescape": "^1.0.1"
  },
  "devDependencies": {
    "cpjax": "^1.1.3",
    "crel": "^3.1.0",
    "crelns": "^1.0.1",
    "cssnano": "^4.1.8",
    "fastn": "^2.12.0",
    "fastn-markdown-component": "^1.2.0",
    "highlight.js": "^9.13.1",
    "konami": "^1.6.2",
    "laidout": "^1.1.0",
    "normalize.css": "^8.0.1",
    "parcel": "^1.11.0",
    "spacetime": "^5.2.1"
  }
}
