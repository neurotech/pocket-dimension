{
  "name": "pocket-dimension",
  "version": "1.0.0",
  "description": "A tiny place to store tiny things.",
  "main": "app.js",
  "scripts": {
    "publish:layer:dynamodbhelper": "aws lambda publish-layer-version --layer-name DynamoDBHelper --description \"Common helpers for DynamoDB\" --license-info 'MIT' --zip-file fileb://${PWD}/build/dynamodbhelper.zip --compatible-runtimes nodejs12.x",
    "build:layer:dynamodbhelper": "rm -rf ./build/dynamodbhelper.zip && cd layers && zip -r ../build/dynamodbhelper.zip ./nodejs/dynamodbHelper/ && cd .."
  },
  "author": "Tim Douglas",
  "license": "MIT",
  "homepage": "https://github.com/neurotech/pocket-dimension",
  "dependencies": {
    "alternator": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "cryptr": "^6.0.1",
    "dion": "^2.0.0",
    "dotenv": "^8.2.0",
    "log-symbols": "^3.0.0",
    "request-data": "^1.4.1",
    "righto": "^6.0.1",
    "sea-lion": "^2.0.2",
    "tiny-json-http": "^7.1.2",
    "unescape": "^1.0.1"
  },
  "devDependencies": {
    "cpjax": "^1.1.3",
    "crel": "^4.2.0",
    "crelns": "^1.0.1",
    "fastn": "^2.14.0",
    "fastn-markdown-component": "^1.2.0",
    "highlight.js": "^9.18.1",
    "konami": "^1.6.2",
    "laidout": "^1.1.0",
    "luxon": "^1.22.0",
    "normalize.css": "^8.0.1",
    "parcel": "^1.12.4"
  }
}
